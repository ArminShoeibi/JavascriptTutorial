


    <script>
          /* Javascript Loops
           * 1) for
           * 2) while
           * 3) do...while
           * 4) for...in
           * 5) for...of
           

           */

         
        for (let i = 0; i < 5; i++) {
            console.log('Hello Armin',i);
        }

        for (let i = 10; i >= 1; i--) {
            console.log(i);
        }

    </script>
